doctype html
html(lang='en')
  head
    // Required meta tags
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1')
    // Bootstrap CSS
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/css/bootstrap.min.css' rel='stylesheet' integrity='sha384-KyZXEAg3QhqLMpG8r+8fhAXLRk2vvoC2f3B09zVXn8CA5QIVfZOJ3BCsw2P0p/We' crossorigin='anonymous')
    title contact us
  body
    nav.navbar.navbar-expand-lg.navbar-light.bg-light
      .container-fluid
        a.navbar-brand(href='') Web Application
        button.navbar-toggler(type='button' data-bs-toggle='collapse' data-bs-target='#navbarNav' aria-controls='navbarNav' aria-expanded='false' aria-label='Toggle navigation')
          span.navbar-toggler-icon
        #navbarNav.collapse.navbar-collapse
          ul.navbar-nav
            li.nav-item
              a.nav-link.active(aria-current='page' href='/') Home
            li.nav-item
              a.nav-link(href='/ourservices') Our Services
            li.nav-item
              a.nav-link(href='/contact_us') Contact us
            li.nav-item
              a.nav-link.disabled(href='#' tabindex='-1' aria-disabled='true')  
    img(src='images/operateurs.jpg')
    form.row.g-4
      .col-sm-3
        label.form-label(for='inputEmail4') Email
        input#inputEmail4.form-control(type='email')
      .col-sm-3
        label.form-label(for='inputPassword4') Password
        input#inputPassword4.form-control(type='password')
      .col-4
        label.form-label(for='inputAddress') Address
        input#inputAddress.form-control(type='text' placeholder='1234 Main St')
      .col-4
        label.form-label(for='inputAddress2') Address 2
        input#inputAddress2.form-control(type='text' placeholder='Apartment, studio, or floor')
      .col-sm-3
        label.form-label(for='inputCity') City
        input#inputCity.form-control(type='text')
      .col-sm-2
        label.form-label(for='inputState') State
        select#inputState.form-select
          option(selected='') Choose...
          option ...    
      .col-8
        .form-check
          input#gridCheck.form-check-input(type='checkbox')
          label.form-check-label(for='gridCheck')
            | Check me out
      .col-8
        button.btn.btn-primary(type='submit') Sign in
